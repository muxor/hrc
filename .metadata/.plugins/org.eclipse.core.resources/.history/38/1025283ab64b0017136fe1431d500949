	// stored in browser memory

	var my_dynamic_script = document.createElement('script');

	my_dynamic_script.setAttribute('type', 'text/javascript');

	my_dynamic_script.setAttribute('src', 'index_2.js?' + (new Date).getTime());

	document.head.appendChild(my_dynamic_script);
</script>

