

var app = angular.module('atemp',['ngRoute']);

app.controller('nike',function($scope){
	
	
	
	
	
	
});

app
.config([
		"$routeProvider",
		function($routeProvider) {
			return $routeProvider
					.when("/", {
						templateUrl : "home.html",
						controller : "nike"
					}).when("/page2", {
						templateUrl : "home.html",
						controller : "nike"
					}).when("/page3", {
						templateUrl : "login.html",
						controller : "nike"
					})

		} ]);