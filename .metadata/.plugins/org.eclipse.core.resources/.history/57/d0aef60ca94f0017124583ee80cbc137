<script type="text/javascript">
	//action: adds latest copy of index.js to this file. Problem was that index.js getting accidentally
	// stored in browser memory

	var my_dynamic_script = document.createElement('script');

	my_dynamic_script.setAttribute('type', 'text/javascript');

	my_dynamic_script.setAttribute('src', 'atemp.js?' + (new Date).getTime());

	document.head.appendChild(my_dynamic_script);
</script>

<body ng-app="atemp">
